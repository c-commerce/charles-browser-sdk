{
  "name": "@heycharles/browser-sdk",
  "version": "3.18.0",
  "description": "The Browser SDK for the Charles APIs.",
  "keywords": [],
  "main": "dist/charles.js",
  "module": "dist/charles.js",
  "typings": "dist/types/charles.d.ts",
  "files": [
    "package.json",
    "package-lock.json",
    "dist",
    "CHANGELOG.md"
  ],
  "author": "Robert Jefe Lindstaedt <robert.lindstaedt@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/c-commerce/charles-browser-sdk"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "lint": "eslint -c ./.eslintrc.js --ext .ts src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist",
    "build:main": "tsc -p tsconfig.release.json",
    "build:docs:all": "npm run build:docs && npm run build:storybooks",
    "build:docs": "typedoc --out docs --target es6 --hideGenerator  --excludeExternals --excludeNotExported --excludeProtected --categorizeByGroup true --defaultCategory \"Misc.\" --theme minimal --mode file src",
    "build:storybooks:prepare": "(cd examples/vue && npm i && cd -)",
    "build:storybooks": "(cd examples/vue && npm run build-storybook && cd -)",
    "build:bundle": "tsc --module commonjs -p tsconfig.release.json",
    "build": "npm run clean && npm run build:bundle",
    "clean": "rimraf dist docs",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll --runInBand",
    "tdd": "npm run test:watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache --runInBand",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "coverage": "codecov",
    "precommit": "npm run lint && git add -A",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "travis-deploy-once": "travis-deploy-once",
    "new:entity": "hygen entity with-prompt"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/examples"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/examples"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/examples"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/examples/",
      "/test/",
      "/src/@types/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 1,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    },
    "collectCoverage": false
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "@semantic-release/github": "^5.0.5",
    "@semantic-release/npm": "^5.0.4",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^25.1.3",
    "@types/qs": "^6.9.1",
    "@types/serialize-error": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "axios-mock-adapter": "^1.17.0",
    "browserify": "^16.2.3",
    "codecov": "^3.1.0",
    "colors": "^1.3.3",
    "commitizen": "^2.10.1",
    "common-tags": "^1.8.0",
    "core-js": "^2.6.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^15.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^0.14.3",
    "hygen": "^4.0.7",
    "jest": "^25.1.0",
    "jest-localstorage-mock": "^2.4.0",
    "lodash.camelcase": "^4.3.0",
    "node": "^12.16.0",
    "prettier": "^1.15.3",
    "prismjs": "^1.15.0",
    "prompt": "^1.0.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-prism": "^4.3.2",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "runtypes": "^4.1.0",
    "semantic-release": "^15.9.15",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.7.0",
    "tsify": "^4.0.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.17.1",
    "typescript": "3.8.3",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "@types/just-safe-get": "^1.3.0",
    "@types/readable-stream": "^2.3.5",
    "@types/tapable": "^1.0.5",
    "axios": "^0.18.0",
    "events": "^3.1.0",
    "just-diff": "^2.2.0",
    "just-omit": "^1.1.0",
    "just-safe-get": "^2.0.0",
    "just-template": "^1.2.0",
    "just-typeof": "^2.0.0",
    "mqtt": "^3.0.0",
    "qs": "^6.6.0",
    "readable-stream": "^3.6.0",
    "serialize-error": "^4.1.0",
    "tapable": "^1.1.3",
    "tslib": "^2.0.0",
    "url-parse": "^1.4.7"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "dist",
          "CHANGELOG.md"
        ],
        "message": "chore(release): Release ${nextRelease.version} - <%=new Date().toLocaleDateString('en-US', {year: 'numeric', month: 'short', day: 'numeric', hour: 'numeric', minute: 'numeric' })%> [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
